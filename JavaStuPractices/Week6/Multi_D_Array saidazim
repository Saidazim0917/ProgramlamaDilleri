package javase;

public class Multi_D_Array {

    public static void main(String[] args) {
        int[][] matrix ={{2,4,6,8}, {3,5,7,9}};

        for(int i = 0; i<matrix.length;i++){
            for(int j=0; j<matrix[i]. length;j++){
                System.out.println(matrix[i][j]+" ");
            }
        }
    }
}
